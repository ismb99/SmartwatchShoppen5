@model IEnumerable<OrderHeader>

<div class="container p-5">
    <div class="m-3">
        <h2 class="text-primary">Order historik</h2>
    </div>
    <table class="table table-hover table-striped">
        <thead>
            <tr>
                <th>Namn</th>
                <th>Adress</th>
                <th>Stad</th>
                <th>Postort</th>
                <th>Telnr</th>
                <th>kostnad</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

                <tr class="table-primary">
                    <td>@item.Name</td>
                    <td>@item.StreetAdress</td>
                    <td>@item.City</td>
                    <td>@item.PostTown</td>
                    <td>@item.PhoneNumber</td>
                    <td>@item.OrderTotal.ToString("c")</td>


                    <td>
                        <div class="w-75 btn-group" role="group">
                            @*<a asp-controller="Product" asp-action="CreateOrUpdate" asp-route-id="@item.Id" class="btn btn-success">Edit</a>*@
                            @*<a asp-controller="Product" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>*@
                        </div>
                    </td>
                </tr>

            }

        </tbody>
    </table>
</div>